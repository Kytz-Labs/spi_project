			font-family: Instrument Sans;
			font-size: 15px;
			font-weight: 500;
			line-height: 24px;
			text-align: left;
		}
		.modules > li {
			font-family: Instrument Sans;
			font-size: 15px;
			font-weight: 500;
			line-height: 24px;
			text-align: left;
		}
		.background-effect{
			content: "";
			position: absolute;
			width:100%;
			inset:0;
			top:10%;
		}

        .career_description {
            font-family: Instrument Sans;
            font-size: 18px;
            font-weight: 500;
            line-height: 28.8px;
            text-align: center;
            text-underline-position: from-font;
            text-decoration-skip-ink: none;
            color: #FFFFFFCC;
        }

		.job-title{
			font-family: Instrument Sans;
			font-size: 36px;
			font-weight: 600;
			line-height: 43.2px;
			text-align: left;
			text-underline-position: from-font;
			text-decoration-skip-ink: none;
			color: #B5DB00;
		}

		.post-date{
			font-family: Instrument Sans;
			font-size: 18px;
			font-weight: 400;
			line-height: 28.8px;
			text-align: justified;
			text-underline-position: from-font;
			text-decoration-skip-ink: none;
			color: #FFFFFFCC;
		}

		.shrt_content{
			font-family: Instrument Sans;
			font-size: 16px;
			font-weight: 500;
			line-height: 19.52px;
			text-align: left;
			text-underline-position: from-font;
			text-decoration-skip-ink: none;
			color: #FFFFFF;
		}

