			font-family: Instrument Sans;
			font-size: 36px;
			font-weight: 600;
			line-height: 46.8px;
			letter-spacing: -0.03em;
			text-align: left;
			text-underline-position: from-font;
			text-decoration-skip-ink: none;
			color: #FFFFFF;
		}

		/* File Upload Container */
		.file-upload {
			border: 2px solid #555;
			border-radius: 8px;
			background-color: #333;
			padding: 20px;
			width: 400px;
			text-align: center;
			position: relative;
		}

		/* Upload Label */
		.upload-label {
			display: block;
			cursor: pointer;
			color: #ddd;
		}

		.upload-label:hover {
			color: #fff;
		}

		/* Upload Text */
		.upload-text {
			font-family: Instrument Sans;
			font-size: 18px;
			font-weight: 700;
			line-height: 28.8px;
			text-align: center;
			text-underline-position: from-font;
			text-decoration-skip-ink: none;
		}	

		/* Accepted Files Text */
		.accepted-files {
			margin-top: 10px;
			font-size: 12px;
			color: #aaa;
		}

		/* Hidden File Input */
		.file-input {
			display: none;
		}

		@media (max-width: 600px) {
			.post{
				width: 100%;
			}
			.post_title{
				font-size: 24px;
			}
			.blogs-title{
				font-size: 32px;
			}
			.blogMain{
				background-color: #292d6b;
				color: white;
			}
			.site-title-layer::before{
				height: 435px;
			}
            .home_title{
                font-size: 40px;
                line-height:50.8px;
            }
			.background-effect{
				display:none;
			}
		}
