        }
        
        .what-we-do-description{
            font-size:14px;line-height:21px;color:rgba(255,255,255,.8);
        }
            
        .learn-m-btn{
            font-size:16px;line-height:19.84px;color:#fff;
        }
    
		@media (max-width: 600px) {
			.post{
				width: 100%;
			}
			.post_title{
