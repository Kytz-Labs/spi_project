]

const special_services = [
	{ image: "/spl_services/spc1.webp", text : "Improve Customer / Employee Experience" },
	{ image: "/spl_services/spc2.webp", text : "Increase Revenue" },
	{ image: "/spl_services/spc3.webp", text : "Reduce Costs" },
	{ image: "/spl_services/spc4.webp", text : "Increase Operational Efficiency / Productivity" },
