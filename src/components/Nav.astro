---
import HeaderLink from './HeaderLink.astro';
import TransBtn from "./button/TransBtn.astro";

const menuList = [
		{ name : "Home", url : "/", ariaLabel: "home page" },
		{ name : "Services", url : "/services", ariaLabel : "services page"}, 
		{ name : "Public Sector", url : "/public-sector", ariaLabel : "public sector page"},
		{ name : "Products", url : "/products", ariaLabel : "products"},
		{ name : "About", url : "/about-us", ariaLabel : "about us" },
		{ name : "Career", url : "/career", ariaLabel : "careers" } 
];
---

<nav class="lg:px-12 px-8 flex flex-row items-center justify-between bg-white z-[50] relative border border-b-[1.5px]">
	<div class="flex-1 flex items-center ">
		<div class="pr-5">
			<a href="/">
				<img src="/logos/spi.webp" width="96px" height="42.98px" />
			</a>
		</div>
		<div class="internal-links ps-5 lg:block hidden">
			{
				menuList.map((data, index) => (
					<div class={ (index == 0) ? "inline-block px-4 border-s-2 leading-[10px] group" : "inline-block px-4 leading-[10px] group" }>
						<HeaderLink href={data.url} aria-label={data.ariaLabel}>
							{data.name}
						</HeaderLink>
						{(index == 1) ? <div class="downarrow absolute group-hover:block group-hover:pt-8 hidden">
