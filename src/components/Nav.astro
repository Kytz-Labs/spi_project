</nav>
<style>
	.aboutarrow{
		display:none;
	}
	.activeAbout{
		display:block;
	}
	.prouctarrow{
		display:none;
	}

	.activeProduct{
		display:block;
	}

	.mobilearrow{
		display: none;
	}
	.activeClass{
		display:block;
	}
	.downarrow a:hover,
	a.active {
	font-weight: bolder;
	color: #006B9E;
	}

	.social-links,
	.social-links a {
		display: flex;
        align-items:center;
	}
	/* navbar */
    #sideopen_D{
        position:absolute;
        visibility: hidden;
        cursor: pointer;
    }
    .sidebox_D{
        display: none;
        position:static;
        transition: all 2s ease-in-out;
        z-index:1;
    }
    #sideopen_D:checked ~ .sidebox_D{
        display: block;
        position: absolute;
        left:0;
        top: 40px;
        width:100%;
    }
    .sidebar_dropdown{
        padding: 1rem;
        border-radius: 10px;
        background-color: black;
        margin: 1rem;
        box-shadow: 1px 1px 5px gray;
        background-color:white;
    }
    .mblist{
        padding: 1rem;
    }
    .mblist:hover{
        border-bottom: 1px solid #0DC195;
    }

	.who{
		font-family: Instrument Sans;
		font-size: 16px;
		font-weight: 400;
		line-height: 19.52px;
		text-align: left;
		text-underline-position: from-font;
		text-decoration-skip-ink: none;
		color: #444444;
	}
	.who-description{
		font-family: Instrument Sans;
		font-size: 12px;
		font-weight: 400;
		line-height: 14.64px;
		text-align: left;
		text-underline-position: from-font;
		text-decoration-skip-ink: none;
		color: #444444;
	}
	@media (max-width: 600px) {
		.social-links {
			display: none;
		}
		h1{
			font-size: 36px;
			line-height: 50px;
		}
		.site-title-layer::after {
			right:-122.5%;
		}
	}
</style>
<script client:visible>

	window.onscroll = () => {
		
		// nav menu functionality
		var nav = document.querySelector("nav");
		if(window.scrollY > 600){
				nav.style.width="100%";
				nav.style.left=0;
				nav.style.position="fixed";
				nav.style.zIndex="21";
				nav.style.left=0;
		}else{
			nav.style.width="100%";
				nav.style.left=0;
				nav.style.position="relative";
				nav.style.zIndex="21";
				nav.style.left=0;
		}
		if(window.scrollY < 500){
			if(window.innerWidth > 600) {
					nav.style.width="100%";
					nav.style.position="relative !important";
					nav.style.zIndex="21";
			}else {
					nav.style.width="100%";
					nav.style.position="relative !important";
					nav.style.zIndex="21"; 
			}
		}
	}

	var arrow = document.querySelector(".arrow_nav");
	var downarrow = document.querySelector(".mobilearrow");
	var arrow_product = document.querySelector(".arrow_product");
	var prodduct_arrow = document.querySelector(".prouctarrow");
	var arrow_about = document.querySelector(".arrow_about");
	var newarrow = document.querySelector(".aboutarrow");
	
	arrow.onclick=() => {
		downarrow.classList.toggle("activeClass");
	}

	arrow_product.onclick = () => {
		prodduct_arrow.classList.toggle("activeProduct");
	}

	arrow_about.onclick = () => {
		console.log(newarrow);
		newarrow.classList.toggle("activeAbout");
	}
</script>
