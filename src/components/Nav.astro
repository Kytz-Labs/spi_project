                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services/transportation-capabilities">Transportation Capabilities</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services/application-management">Application Management</a></div>
                        <div class="subnavlistItem p-4 pt-3 pt-0 bg-white"><a href="/services/digital-transformation">Digital Transformation</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services/business-intelligence">Business Intelligence/Analytics</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services/it-staffing">Enterprise Infrastructure</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services/it-staffing">IT Staffing</a></div>
						<div class="subnavlistItem p-4 pt-3 bg-white"><a href="/about-us#partnerships">Appian Partnership</a></div>
						<div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services/professionals-it-services">Professional IT Services</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/services">All Services</a></div>
                    </div> : ""}
					{(index == 3) ? <div class="downarrow absolute group-hover:block group-hover:pt-8 hidden">
                        <div class="subnavlistItem p-4 bg-white"><a href="/products/safe-connect">SafeConnect</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/products/safe-school">SafeSchool</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/products/enviro-connect">EnviroConnect</a></div>
                        <div class="subnavlistItem p-4 pt-3 bg-white"><a href="/products/inner-LMS">E-LMS</a></div>
                        <div class="subnavlistItem p-4 pt-3 pt-0 bg-white"><a href="/products/permit-connect">PermitConnect</a></div>
                    </div> : ""}
					{(index == 4) ? <div class="downarrow absolute group-hover:block hidden w-full left-0">
                        <div class="lg:grid grid-cols-5 hidden group-hover:mt-6 bg-white ">
							<a href="/about-us" target="_blank">
								<div class="flex-1 border border-black p-4 h-full">
									<div class="who pb-4">Who we are</div>
									<div class="who-description">SPI provides SaaS solutions in Safety and Environmental Compliance, driven by our core values.</div>
									<div class="pt-8">
										<img src="/Nav/aboutNav.webp" width="153px" height="128px" />
									</div>
								</div>
							</a>
							<a href="/about-us#partnerships" target="_blank">
								<div class="flex-1 border border-black p-4 h-full">
									<div class="who pb-4">Partnership</div>
									<div class="who-description">Appian is a software company that automates business processes.</div>
									<div class="pt-8">
										<img src="/Nav/partnershipNav.webp" width="153px" height="128px" />
